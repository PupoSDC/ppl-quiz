{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "esnext.asynciterable"],
    "module": "commonjs",
    "outDir": "./dist",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": true
  },
  "include": ["./src/*"]
}
